@go.Package { name = "github.com/zackarysantana/velocity/internal/pkl/velocity/constructs/runtime" }
module runtime_construct

import "package://pkg.pkl-lang.org/pkl-go/pkl.golang@0.5.3#/go.pkl" 

abstract class BaseRuntime {
    name: String
    env: Mapping<String, String>?
}

class DockerRuntime extends BaseRuntime {
    image: String
}

class MachineRuntime extends BaseRuntime {
    machine: String
}

typealias Runtime = DockerRuntime|MachineRuntime

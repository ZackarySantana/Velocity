@go.Package { name = "github.com/zackarysantana/velocity/gen/pkl/prebuilts/build" }
module build_prebuilt

import "package://pkg.pkl-lang.org/pkl-go/pkl.golang@0.5.3#/go.pkl" 

import "../primitives/command.pkl"
import "../primitives/names.pkl"

class _request extends command.PrebuiltCommand {
    function validate(n: names.Names): String? = if (n.Builds.contains(this.params["build"])) null else "using invalid build name '\(this.params["build"])' in build.create"
}

function create(buildName: String) = new _request {
    prebuilt = "build.create"
    params {
        ["build"] = buildName
    }
}
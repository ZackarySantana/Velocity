@go.Package { name = "github.com/zackarysantana/velocity/gen/pkl/constructs/build" }
module build_construct

import "package://pkg.pkl-lang.org/pkl-go/pkl.golang@0.5.3#/go.pkl" 

import "../primitives/command.pkl"

class Build {
    name: String
    build_runtime: String
    output: String

    output_runtime: String?
    output_command: String?

    commands: Listing<command.Command>

    function getRuntimeNames(): List<String> = List(this.build_runtime, this.output_runtime ?? null).filterNonNull()
}
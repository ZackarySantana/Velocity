@go.Package { name = "github.com/zackarysantana/velocity/gen/pkl/constructs/deployment" }
module deployment_construct

import "package://pkg.pkl-lang.org/pkl-go/pkl.golang@0.5.3#/go.pkl" 

import "../primitives/command.pkl"

class Deployment {
    name: String
    runtime: String

    workflows: Listing<String>?

    commands: Listing<command.Command>

    function getWorkflowNames(): List<String> = workflows?.toList() ?? List()
    function getRuntimeNames(): List<String> = List(runtime)
}
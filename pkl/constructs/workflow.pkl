@go.Package { name = "github.com/zackarysantana/velocity/internal/pkl/velocity/constructs/workflow" }
module workflow_construct

import "package://pkg.pkl-lang.org/pkl-go/pkl.golang@0.5.3#/go.pkl" 

class WorkflowGroup {
    name: String
    runtimes: Listing<String>
    tests: Listing<String>
}

class Workflow {
    name: String
    groups: Listing<WorkflowGroup>

    function getRuntimeNames(): List<String> = this.groups.toList().flatMap((g) -> g.runtimes.toList())
    function getTestNames(): List<String> = this.groups.toList().flatMap((g) -> g.tests.toList())
}